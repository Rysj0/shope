/* pages/my/my.wxss */

.personal_info{
    padding:20px 0;
    background-color: #fff;
}
.photo_wrap{
    text-align: center;
}
.photo_wrap image{
    width: 90px;
    height: 90px;
    border-radius: 45px;
}
.nickname{
    margin-top: #fff;
    text-align: center;
    color: #696969;
}
.wode_item_wrap{
    background-color: #fff;
    margin-top: 10px;
    font-size: 14px;
    border-top: 1px solid #e5e5e5;
}
.wode_item{
    font-size: 14px;
   padding: 0 10px;
   height: 45px;
   line-height: 45px;
    border-bottom: 1px solid #e5e5e5;
    position: relative;
}
.wode_item image{
    width: 20px;
    height: 20px;
    position: absolute;
    top: 12px;
    right: 10px;
}
.wode_out{
    height: 45px;
    line-height: 45px;
    text-align: center;
    background-color: #fff;
    margin-top: 10px;
    border-bottom: 1px solid #e5e5e5;
    border-top: 1px solid #e5e5e5;

}